# REQUIRES: x86
# RUN: llvm-mc -filetype=obj -triple=x86_64-unknown-linux /dev/null -o %t.o
# RUN: not ld.lld -shared %t.o -o %t --script %s 2>&1 | FileCheck -strict-whitespace %s

UNKNOWN_TAG {
  .text : { *(.text) }
  .keep : { *(.keep) }
  .temp : { *(.temp) }
}

CHECK:      5: unknown directive: UNKNOWN_TAG
CHECK-NEXT: >>> UNKNOWN_TAG {
CHECK-NEXT: >>> ^
