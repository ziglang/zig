      {#code_begin|exe_err#}
const std = @import("std");

pub fn main() void {
    const number = getNumberOrFail() catch unreachable;
    std.debug.warn("value: {}\n", number);
}

fn getNumberOrFail() !i32 {
    return error.UnableToReturnNumber;
}
