      {#code_begin|test|null_terminated_array#}
const std = @import("std");
const expect = std.testing.expect;

test "null terminated array" {
    const array = [_:0]u8 {1, 2, 3, 4};

    expect(@TypeOf(array) == [4:0]u8);
    expect(array.len == 4);
    expect(array[4] == 0);
}
