const std = @import("std");

const Point = struct {
    x: u32,
    y: u32,

    pub var z: u32 = 1;
};

test "field access by string" {
    const expect = std.testing.expect;
    var p = Point{ .x = 0, .y = 0 };

    @field(p, "x") = 4;
    @field(p, "y") = @field(p, "x") + 1;

    expect(@field(p, "x") == 4);
    expect(@field(p, "y") == 5);
}

test "decl access by string" {
    const expect = std.testing.expect;

    expect(@field(Point, "z") == 1);

    @field(Point, "z") = 2;
    expect(@field(Point, "z") == 2);
}
