      {#code_begin|test#}
const std = @import("std");
const expect = std.testing.expect;

test "coerce to optionals wrapped in error union" {
    const x: anyerror!?i32 = 1234;
    const y: anyerror!?i32 = null;

    expect((try x).? == 1234);
    expect((try y) == null);
}
      {#code_end#}
