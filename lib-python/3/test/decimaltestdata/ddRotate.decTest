ddrot078 rotate -0.000              +9   ->  -0.000
ddrot079 rotate -0.000              -9   ->  -0.000
ddrot080 rotate -0E+10              +9   ->  -0E+10
ddrot081 rotate -0E+10              -9   ->  -0E+10

-- Nmax, Nmin, Ntiny
ddrot141 rotate  9.999999999999999E+384     -1  -> 9.999999999999999E+384
