-- Krah examples
precision:   34
maxExponent: 999999999
minExponent: -999999999
-- integer overflow in 3.61 or earlier
scbx164 scaleb  1E-999999999  -1200000000  -> NaN Invalid_operation
-- out of range
scbx165 scaleb  -1E-999999999  +1200000000  -> NaN Invalid_operation
